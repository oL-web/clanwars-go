{
  "name": "clanwars-go",
  "version": "0.1.0",
  "description": "ClanWars GO is a MERN stack game project using Google Maps and geolocation APIs to create an experience similiar to games such as Pokemon GO.",
  "author": "ol-web",
  "scripts": {
    "dev-front": "parcel src/index.html",
    "dev-server": "nodemon --ignore dist/* --ignore src/* --ignore .cache/* --inspect --require dotenv/config server.js",
    "dev": "concurrently \"parcel watch src/index.html\" \"npm run dev-server\"",
    "server": "node --require dotenv/config server.js",
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.html --no-source-maps  --public-url \".\"",
    "postbuild": "imagemin dist/* --out-dir=dist && workbox generateSW"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "concurrently": "^3.5.1",
    "connect-mongo": "^2.0.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-rate-limit": "^3.3.2",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "imagemin-cli": "^3.0.0",
    "mongoose": "^5.4.2",
    "nodemon": "^1.17.5",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-url-loader": "^1.2.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "rate-limit-mongo": "^1.0.3",
    "rimraf": "^2.6.2",
    "workbox-cli": "^3.2.0"
  },
  "dependencies": {
    "random-number-in-range": "^1.0.2",
    "lodash": "^4.17.11",
    "random-color": "^1.0.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-jsx": "^2.2.6",
    "superagent": "^4.1.0",
    "times-in-milliseconds": "0.0.3"
  }
}